@model List<Employee>

<table class="table table-hover">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
            <th scope="col">Title</th>
            <th scope="col">Age</th>
        </tr>
    </thead>
        <tbody>
        @foreach (var item in Model)
        { 
        <tr>
               <th>@item.Id</th>
               <td>@item.Name</td>
               <td>@item.Title</td>
               <td>@item.Age</td>
        </tr>
        }
        </tbody>
</table>
